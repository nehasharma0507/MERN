#include <math.h>
#include <stdio.h>
#include <string.h>

int main() 
{
    int n ,array[20];
    int i;
    int largest;
    int second_largest;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&array[i]);
    }
    largest=array[0];
    second_largest=array[0];
    for(i=0;i<n;i++){
        if(largest<array[i]){
            largest=array[i];
        }
    }
    for(i=0;i<n;i++){
        if(second_largest<array[i] && array[i]!=largest){
            second_largest=array[i];
        }
    }
    printf("%d\n%d",largest,second_largest);
    
    return 0;
}
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n,sum=0,firstDigit, lastDigit;
    
    scanf("%d",&n);
    lastDigit = n%10;
    while(n>=10)
    {
        n = n/10;
    }
    firstDigit = n;
    sum= firstDigit + lastDigit;
    printf("%d",sum);
    return 0;
}


