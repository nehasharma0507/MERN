#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main()
 {
    int findpeak(int arr[],int n);
    int peakIndex=0;
    int i,n,arr[i];
       
       for(i=1;i<n;i++)
    {
        if (arr[i] > arr[peakIndex]){
            peakIndex=i;
        }
        return arr[peakIndex];
    }
}
int main ()
{
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int peak=findpeak(arr,n);
    printf("%d\n",peak);
    return 0;
}
